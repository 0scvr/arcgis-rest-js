{
  "name": "@esri/arcgis-rest-common",
  "version": "1.15.2",
  "description": "Shared utility methods and types for @esri/arcgis-rest",
  "main": "dist/node/index.js",
  "unpkg": "dist/umd/common.umd.js",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:node && npm run build:umd && npm run build:esm",
    "build:esm": "tsc -p ./tsconfig.json --module es2015 --outDir ./dist/esm --declaration",
    "build:umd": "rollup -c ../../umd-base-profile.js && rollup -c ../../umd-production-profile.js",
    "build:node": "tsc -p ./tsconfig.json --module commonjs --outDir ./dist/node",
    "dev:esm": "tsc -w --module es2015 --outDir ./dist/esm --declaration",
    "dev:umd": "rollup -w -c ../../umd-base-profile.js",
    "dev:node": "tsc -w --module commonjs --outDir ./dist/node"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/arcgis-rest-js.git"
  },
  "contributors": [
    "John Gravois <john@esri.com>"
  ],
  "bugs": {
    "url": "https://github.com/Esri/arcgis-rest-js/issues"
  },
  "homepage": "https://github.com/Esri/arcgis-rest-js#readme"
}
